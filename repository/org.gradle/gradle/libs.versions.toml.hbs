[versions]
{{#each _project.versions.entries }}
{{ key }} = "{{ value }}"
{{/each}}

[libraries]
{{#each _project.libraries.entries }}
{{ key }} = { module = "{{ value.module }}", version.ref = "{{ value.version.ref }}" }
{{/each}}

[plugins]
{{#each _project.gradle.plugins}}
{{ name }} = { id = "{{ id }}", version = "{{ version }}" }
{{/each}}